// cstddef standard header
#pragma once
#ifndef _CSTDDEF_
#define _CSTDDEF_
#include <yvals.h>

#ifdef _STD_USING
 #undef _STD_USING
  #include <stddef.h>
 #define _STD_USING

#else /* _STD_USING */
 #include <stddef.h>
#endif /* _STD_USING */

 #if _GLOBAL_USING && !defined(RC_INVOKED)
_STD_BEGIN
using _CSTD ptrdiff_t; using _CSTD size_t;
_STD_END
 #endif /* _GLOBAL_USING */

 #if !defined(RC_INVOKED)
_STD_BEGIN
typedef double max_align_t;	// most aligned type
_STD_END
 #endif /* !defined(RC_INVOKED) */
#endif /* _CSTDDEF_ */

