		// CLASS numeric_limits<unsigned long>
template<> class numeric_limits<unsigned long>
	: public _Num_int_base
	{	// limits for type unsigned long
public:
	typedef unsigned long _Ty;

	static _CONST_FUN _Ty (min)() _THROW0()
		{	// return minimum value
		return (0);
		}

	static _CONST_FUN _Ty (max)() _THROW0()
		{	// return maximum value
		return (ULONG_MAX);
		}

	static _CONST_FUN _Ty lowest() _THROW0()
		{	// return most negative value
		return ((min)());
		}

	static _CONST_FUN _Ty epsilon() _THROW0()
		{	// return smallest effective increment from 1.0
		return (0);
		}

	static _CONST_FUN _Ty round_error() _THROW0()
		{	// return largest rounding error
		return (0);
		}

	static _CONST_FUN _Ty denorm_min() _THROW0()
		{	// return minimum denormalized value
		return (0);
		}

	static _CONST_FUN _Ty infinity() _THROW0()
		{	// return positive infinity
		return (0);
		}

	static _CONST_FUN _Ty quiet_NaN() _THROW0()
		{	// return non-signaling NaN
		return (0);
		}

	static _CONST_FUN _Ty signaling_NaN() _THROW0()
		{	// return signaling NaN
		return (0);
		}

	_STCONS(bool, is_signed, false);
	_STCONS(int, digits, CHAR_BIT * sizeof (unsigned long));
	_STCONS(int, digits10, CHAR_BIT * sizeof (unsigned long)
		* 301L / 1000);
	};

