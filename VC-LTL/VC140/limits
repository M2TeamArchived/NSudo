		// CLASS numeric_limits<float>
template<> class numeric_limits<float>
	: public _Num_float_base
	{	// limits for type float
public:
	typedef float _Ty;

	static _CONST_FUN _Ty (min)() _THROW0()
		{	// return minimum value
		return (_FLT_MIN);
		}

	static _CONST_FUN _Ty (max)() _THROW0()
		{	// return maximum value
		return (_FLT_MAX);
		}

	static _CONST_FUN _Ty lowest() _THROW0()
		{	// return most negative value
		return (-(max)());
		}

	static _CONST_FUN _Ty epsilon() _THROW0()
		{	// return smallest effective increment from 1.0
		return (_FLT_EPSILON);
		}

	static _CONST_FUN _Ty round_error() _THROW0()
		{	// return largest rounding error
		return (0.5F);
		}

	static _CONST_FUN _Ty denorm_min() _THROW0()
		{	// return minimum denormalized value
		return (_FLT_TRUE_MIN);
		}

	static _CONST_FUN _Ty infinity() _THROW0()
		{	// return positive infinity
		return (_INF_LIMIT);
		}

	static /* _CONST_FUN */ _Ty quiet_NaN() _THROW0()	// TRANSITION
		{	// return non-signaling NaN
		return (_CSTD _FNan._Float);
		}

	static /* _CONST_FUN */ _Ty signaling_NaN() _THROW0()	// TRANSITION
		{	// return signaling NaN
		return (_CSTD _FSnan._Float);
		}

	_STCONS(int, digits, FLT_MANT_DIG);
	_STCONS(int, digits10, FLT_DIG);

	_STCONS(int, max_digits10, 2 + FLT_MANT_DIG * 301L / 1000);

	_STCONS(int, max_exponent, (int)FLT_MAX_EXP);
	_STCONS(int, max_exponent10, (int)FLT_MAX_10_EXP);
	_STCONS(int, min_exponent, (int)FLT_MIN_EXP);
	_STCONS(int, min_exponent10, (int)FLT_MIN_10_EXP);
	};

